@model CapoteSolution.Web.Models.ViewModels.TonerDisplayVM

@{
    ViewData["Title"] = "Detalles del Toner";
}

<div class="card">
    <div class="card-header bg-primary text-white">
        <h3>@ViewData["Title"]</h3>
    </div>
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-4">Modelo:</dt>
            <dd class="col-sm-8">@Model.Model</dd>
           

            <dt class="col-sm-4">Rendimiento:</dt>
            <dd class="col-sm-8">@Model.Yield copias</dd>                        
           
        </dl>
    </div>
    <div class="card-footer">
        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Editar</a>
        <a asp-action="Index" class="btn btn-secondary">Volver al Listado</a>
    </div>
</div>
